<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/transparent">

    <include
        android:id="@+id/action_bar"
        layout="@layout/action_bar2_layout"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="626dp"
        android:layout_marginStart="128dp" />
    <com.syberos.shuili.view.CustomScrollView
        android:layout_below="@+id/action_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_marginBottom="@dimen/_10dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ll_check_object"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_10dp"
                android:layout_marginLeft="@dimen/_10dp"
                android:paddingLeft="@dimen/_5dp"
                android:paddingRight="@dimen/_5dp"
                android:layout_marginTop="@dimen/padding_5"
                android:background="@drawable/dialog_bg_shape"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rl__check_object_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/_10dp">
                    <LinearLayout
                        android:id="@+id/ll_check_object_title"
                        android:layout_width="@dimen/_3dp"
                        android:layout_height="@dimen/_15dp"
                        android:background="@color/button_login_normal"
                        android:layout_gravity="center_vertical"
                        android:orientation="vertical"/>
                    <TextView
                        android:layout_toEndOf="@+id/ll_check_object_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="检查小组信息"
                        android:layout_marginStart="@dimen/_2dp"
                        android:textColor="@color/button_login_normal"/>
                    <TextView
                        android:id="@+id/tv_check_object_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="@dimen/_10dp"/>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/ll_child_groups_container"
                    android:divider="@drawable/div_shape"
                    android:padding="@dimen/_6dp"
                    android:showDividers="middle"
                    android:orientation="vertical">
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/gray_bb"
                    android:layout_marginTop="@dimen/_2dp"/>
                <RelativeLayout
                    android:id="@+id/rl_group_unit"
                    android:layout_width="match_parent"
                    android:layout_height= "wrap_content"
                    android:minHeight="@dimen/_40dp"
                    android:layout_marginTop="@dimen/_5dp">
                    <TextView
                        android:id="@+id/tv_group_unit_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="组长单位:"/>

                    <TextView
                        android:id="@+id/tv_group_unit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/tv_group_unit_label"
                        android:layout_marginStart="@dimen/padding_20"
                        android:layout_centerVertical="true"
                        android:text= "A大队"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/gray_bb"
                    android:layout_marginTop="@dimen/_2dp"/>
                <RelativeLayout
                    android:id="@+id/rl_member_unit"
                    android:layout_width="match_parent"
                    android:layout_height= "wrap_content"
                    android:visibility="gone"
                    android:minHeight="@dimen/_40dp"
                    android:layout_marginTop="@dimen/_5dp">
                    <TextView
                        android:id="@+id/tv_member_unit_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="组员单位:"/>

                    <TextView
                        android:id="@+id/tv_member_unit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/tv_member_unit_label"
                        android:layout_marginStart="@dimen/padding_20"
                        android:layout_centerVertical="true"
                        android:text= "朝阳区双桥水管所"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/gray_bb"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_2dp"/>
                <RelativeLayout
                    android:id="@+id/rl_check_persons"
                    android:layout_width="match_parent"
                    android:layout_height= "wrap_content"
                    android:minHeight="@dimen/_40dp"
                    android:layout_marginTop="@dimen/_5dp">
                    <TextView
                        android:id="@+id/tv_check_person_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="专家姓名:"/>

                    <TextView
                        android:id="@+id/tv_check_person"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/tv_check_person_label"
                        android:layout_marginStart="@dimen/padding_20"
                        android:layout_centerVertical="true"
                        android:text= "李白、杜甫"/>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/gray_bb"
                    android:layout_marginTop="@dimen/_2dp"/>
                <RelativeLayout
                    android:id="@+id/rl_check_project"
                    android:layout_width="match_parent"
                    android:layout_height= "wrap_content"
                    android:minHeight="@dimen/_40dp"
                    android:layout_marginTop="@dimen/_5dp">
                    <TextView
                        android:id="@+id/tv_check_project_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="检查对象:"/>

                    <TextView
                        android:id="@+id/tv_check_project"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/tv_check_project_label"
                        android:layout_marginStart="@dimen/padding_20"
                        android:layout_centerVertical="true"
                        android:text= "李白、杜甫"/>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/gray_bb"
                    android:layout_marginTop="@dimen/_2dp"/>
                <RelativeLayout
                    android:id="@+id/rl_problem_count"
                    android:layout_width="match_parent"
                    android:layout_height= "wrap_content"
                    android:minHeight="@dimen/_40dp"
                    android:layout_marginTop="@dimen/_5dp">
                    <TextView
                        android:id="@+id/tv_problem_count_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="隐患数量:"/>

                    <TextView
                        android:id="@+id/tv_problem_count"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/tv_problem_count_label"
                        android:layout_marginStart="@dimen/padding_20"
                        android:layout_centerVertical="true"
                        android:text= "李白、杜甫"/>

                </RelativeLayout>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_hidden_object"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_10dp"
                android:layout_marginLeft="@dimen/_10dp"
                android:paddingLeft="@dimen/_5dp"
                android:paddingRight="@dimen/_5dp"
                android:layout_marginTop="@dimen/padding_5"
                android:background="@drawable/dialog_bg_shape"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rl_hiddlen_object_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/_10dp">
                    <LinearLayout
                        android:id="@+id/ll_hiddlen_object_title"
                        android:layout_width="@dimen/_3dp"
                        android:layout_height="@dimen/_15dp"
                        android:background="@color/button_login_normal"
                        android:layout_gravity="center_vertical"
                        android:orientation="vertical"/>
                    <TextView
                        android:layout_toEndOf="@+id/ll_hiddlen_object_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="隐患列表"
                        android:layout_marginStart="@dimen/_2dp"
                        android:textColor="@color/button_login_normal"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/gray_bb"
                    android:layout_marginTop="@dimen/_2dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/ll_hidden_object_container"
                    android:divider="@drawable/div_shape"
                    android:showDividers="middle"
                    android:orientation="vertical">
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </com.syberos.shuili.view.CustomScrollView>
</RelativeLayout>
