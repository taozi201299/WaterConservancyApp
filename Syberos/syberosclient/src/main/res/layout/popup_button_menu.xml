<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ripple="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/v_shadow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#CC000000" />

    <com.syberos.shuili.view.PopupButton.RippleLayout
        android:id="@+id/rl_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="12.0dip"
        android:paddingLeft="24.0dip"
        android:paddingRight="24.0dip"
        android:paddingTop="12.0dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="bottom|end"
        android:alpha="0.9"
        ripple:rv_color="@color/grey_normal"
        ripple:rv_inner_radius="@dimen/popup_button_inner_radius">

        <ImageButton
            android:layout_width="@dimen/popup_button_inner_size"
            android:layout_height="@dimen/popup_button_inner_size"
            android:layout_centerInParent="true"
            android:background="@drawable/selector_pressed_ball"
            android:contentDescription="@null"
            android:scaleType="center"
            android:src="@mipmap/popup_button_add" />
    </com.syberos.shuili.view.PopupButton.RippleLayout>

    <LinearLayout
        android:id="@+id/import_container"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_above="@+id/rl_close"
        android:layout_alignParentEnd="true"
        android:layout_gravity="bottom|end"
        android:gravity="bottom"
        android:orientation="vertical"
        android:paddingBottom="-20.0dip">

        <LinearLayout
            android:id="@+id/ll_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_top"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="-10dp"
                android:singleLine="true"
                android:text="@string/menu_item_new_danger"
                android:textColor="@android:color/white" />

            <com.syberos.shuili.view.PopupButton.RippleLayout
                android:id="@+id/rl_top"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="12.0dip"
                android:paddingLeft="24.0dip"
                android:paddingRight="24.0dip"
                android:paddingTop="12.0dip"
                ripple:rv_color="@color/color_normal"
                ripple:rv_inner_radius="@dimen/popup_button_inner_radius"
                ripple:rv_type="simpleRipple">

                <LinearLayout
                    android:layout_width="@dimen/popup_button_inner_size"
                    android:layout_height="@dimen/popup_button_inner_size"
                    android:layout_centerInParent="true"
                    android:background="@drawable/selector_normal_ball"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:src="@mipmap/popup_button_top" />
                </LinearLayout>
            </com.syberos.shuili.view.PopupButton.RippleLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="-10dp"
                android:singleLine="true"
                android:text="@string/menu_item_new_accident"
                android:textColor="@android:color/white" />

            <com.syberos.shuili.view.PopupButton.RippleLayout
                android:id="@+id/rl_center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="12.0dip"
                android:paddingLeft="24.0dip"
                android:paddingRight="24.0dip"
                android:paddingTop="12.0dip"
                ripple:rv_color="@color/color_normal"
                ripple:rv_inner_radius="@dimen/popup_button_inner_radius"
                ripple:rv_type="simpleRipple">

                <LinearLayout
                    android:layout_width="@dimen/popup_button_inner_size"
                    android:layout_height="@dimen/popup_button_inner_size"
                    android:layout_centerInParent="true"
                    android:background="@drawable/selector_normal_ball"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:src="@mipmap/popup_button_center" />
                </LinearLayout>
            </com.syberos.shuili.view.PopupButton.RippleLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="-10dp"
                android:text="@string/menu_item_new_risk"
                android:textColor="@android:color/white" />

            <com.syberos.shuili.view.PopupButton.RippleLayout
                android:id="@+id/rl_bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="12.0dip"
                android:paddingLeft="24.0dip"
                android:paddingRight="24.0dip"
                android:paddingTop="12.0dip"
                ripple:rv_color="@color/color_normal"
                ripple:rv_inner_radius="@dimen/popup_button_inner_radius"
                ripple:rv_type="simpleRipple">

                <LinearLayout
                    android:layout_width="@dimen/popup_button_inner_size"
                    android:layout_height="@dimen/popup_button_inner_size"
                    android:layout_centerInParent="true"
                    android:background="@drawable/selector_normal_ball"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:src="@mipmap/popup_button_bottom" />
                </LinearLayout>
            </com.syberos.shuili.view.PopupButton.RippleLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_bottom_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_bottom_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="-10dp"
                android:text="@string/menu_item_new_check"
                android:textColor="@android:color/white" />

            <com.syberos.shuili.view.PopupButton.RippleLayout
                android:id="@+id/rl_bottom_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="12.0dip"
                android:paddingLeft="24.0dip"
                android:paddingRight="24.0dip"
                android:paddingTop="12.0dip"
                ripple:rv_color="@color/color_normal"
                ripple:rv_inner_radius="@dimen/popup_button_inner_radius"
                ripple:rv_type="simpleRipple">

                <LinearLayout
                    android:layout_width="@dimen/popup_button_inner_size"
                    android:layout_height="@dimen/popup_button_inner_size"
                    android:layout_centerInParent="true"
                    android:background="@drawable/selector_normal_ball"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:src="@mipmap/popup_button_bottom" />
                </LinearLayout>
            </com.syberos.shuili.view.PopupButton.RippleLayout>

        </LinearLayout>


    </LinearLayout>
</merge>